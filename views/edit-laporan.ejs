<h3>
    <center><%= laporan.nama %></center>
</h3>
<br>
<div class="d-flex justify-content-center">
    <div class="card" style="width: 500px; background-color: white;">
        <div class="card-body">
            <p><%= laporan.date %></p>
            <form method="POST" action="/edit-laporan/<%= laporan._id %>">
                <div class="form-group">
                    <label>Staff</label>
                    <input type="name" class="form-control" id="staff" name="staff" value="<%= laporan.staff %>" required>
                    <input type="hidden" class="form-control" id="staff" name="user" value="<%= laporan.user %>" required>
                    <input type="hidden" class="form-control" id="staff" name="nama" value="<%= laporan.nama %>" required>
                    <input type="hidden" class="form-control" id="staff" name="date" value="<%= laporan.date %>" required>

                  </div>
                <hr>

                <h5>05.00 - 13.00 <small>(Shift 1)</small></h5>
                <div class="form-group">
                    <div class="table-responsive">
                        <table class="table table-striped">
                            <thead>
                              <tr class="table-primary">
                                <th scope="col">Ayam</th>
                                <th scope="col">&emsp;D</th>
                                <th scope="col">&emsp;PA</th>
                                <th scope="col">&emsp;PB</th>
                                <th scope="col">&emsp;S</th>
                              </tr>
                            </thead>
                            <tbody>
                              <tr>
                                <th scope="row"><small>Datang</small></th>
                                <td><input class="form-control" id="d" name="s1DatangD" value="<%= laporan.data.s1.datang.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1DatangPA" value="<%= laporan.data.s1.datang.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1DatangPB" value="<%= laporan.data.s1.datang.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1DatangS" value="<%= laporan.data.s1.datang.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row"><small>Stok Awal</small></th>
                                <td><input class="form-control" id="d" name="s1StokAwalD" value="<%= laporan.data.s1.stokAwal.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1StokAwalPA" value="<%= laporan.data.s1.stokAwal.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1StokAwalPB" value="<%= laporan.data.s1.stokAwal.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1StokAwalS" value="<%= laporan.data.s1.stokAwal.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Stok</th>
                                <td>&emsp;<%= laporan.data.s1.stok.d %></td>
                                <td>&emsp;<%= laporan.data.s1.stok.pa %></td>
                                <td>&emsp;<%= laporan.data.s1.stok.pb %></td>
                                <td>&emsp;<%= laporan.data.s1.stok.s %></td>                              
                              </tr>
                              <tr>
                                <th scope="row"><small>Digoreng</small></th>
                                <td><input class="form-control" id="d" name="s1MasakD" value="<%= laporan.data.s1.masak.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1MasakPA" value="<%= laporan.data.s1.masak.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1MasakPB" value="<%= laporan.data.s1.masak.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1MasakS" value="<%= laporan.data.s1.masak.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row"><small>Fillet</small></th>
                                <td><input class="form-control" id="s" name="s1fillet" value="<%= laporan.data.s1.masak.f %>"></td>
                              </tr>
                              <tr>
                                <th scope="row"><strong style="color: blue;">LAKU</strong></th>
                                <td><input class="form-control" id="d" name="s1LakuD" value="<%= laporan.data.s1.laku.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1LakuPA" value="<%= laporan.data.s1.laku.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1LakuPB" value="<%= laporan.data.s1.laku.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1LakuS" value="<%= laporan.data.s1.laku.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Etalase</th>
                                <td>&emsp;<%= laporan.data.s1.etalase.d %></td>
                                <td>&emsp;<%= laporan.data.s1.etalase.pa %></td>
                                <td>&emsp;<%= laporan.data.s1.etalase.pb %></td>
                                <td>&emsp;<%= laporan.data.s1.etalase.s %></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Freezer</th>
                                <td>&emsp;<%= laporan.data.s1.simpan.d %></td>
                                <td>&emsp;<%= laporan.data.s1.simpan.pa %></td>
                                <td>&emsp;<%= laporan.data.s1.simpan.pb %></td>
                                <td>&emsp;<%= laporan.data.s1.simpan.s %></td>                              
                              </tr>
                            </tbody>
                          </table>
                      </div>
                </div>
                <hr>

