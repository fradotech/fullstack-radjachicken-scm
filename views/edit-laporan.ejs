                                <td><p id="pa" name="s2SimpanPA" value="<%= laporan.data.s2.simpan.pa %>"></td>
                                <td><p id="pb" name="s2SimpanPB" value="<%= laporan.data.s2.simpan.pb %>"></td>
                                <td><p id="s" name="s2SimpanS" value="<%= laporan.data.s2.simpan.s %>"></td>                              
