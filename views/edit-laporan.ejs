                            <thead>
                              <tr class="table-primary">
                                <th scope="col">Ayam</th>
                                <th scope="col">D</th>
                                <th scope="col">PA</th>
                                <th scope="col">PB</th>
                                <th scope="col">S</th>
                              </tr>
                            </thead>
                            <tbody>
                              <tr>
                                <th scope="row">Stok</th>
                                <td><input class="form-control" id="d" name="s1StokD" value="<%= laporan.data.s1.stok.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1StokPA" value="<%= laporan.data.s1.stok.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1StokPB" value="<%= laporan.data.s1.stok.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1StokS" value="<%= laporan.data.s1.stok.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Datang</th>
                                <td><input class="form-control" id="d" name="s1DatangD" value="<%= laporan.data.s1.datang.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1DatangPA" value="<%= laporan.data.s1.datang.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1DatangPB" value="<%= laporan.data.s1.datang.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1DatangS" value="<%= laporan.data.s1.datang.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Digoreng</th>
                                <td><input class="form-control" id="d" name="s1MasakD" value="<%= laporan.data.s1.masak.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1MasakPA" value="<%= laporan.data.s1.masak.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1MasakPB" value="<%= laporan.data.s1.masak.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1MasakS" value="<%= laporan.data.s1.masak.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Laku</th>
                                <td><input class="form-control" id="d" name="s1LakuD" value="<%= laporan.data.s1.laku.d %>"></td>
                                <td><input class="form-control" id="pa" name="s1LakuPA" value="<%= laporan.data.s1.laku.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s1LakuPB" value="<%= laporan.data.s1.laku.pb %>"></td>
                                <td><input class="form-control" id="s" name="s1LakuS" value="<%= laporan.data.s1.laku.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Etalase</th>
                                <td><p id="d" name="s1EtalaseD" value="<%= laporan.data.s1.etalase.d %>"></td>
                                <td><p id="pa" name="s1EtalasePA" value="<%= laporan.data.s1.etalase.pa %>"></td>
                                <td><p id="pb" name="s1EtalasePB" value="<%= laporan.data.s1.etalase.pb %>"></td>
                                <td><p id="s" name="s1EtalaseS" value="<%= laporan.data.s1.etalase.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Freezer</th>
                                <td><p id="d" name="s1SimpanD" value="<%= laporan.data.s1.simpan.d %>"></td>
                                <td><p id="pa" name="s1SimpanPA" value="<%= laporan.data.s1.simpan.pa %>"></td>
                                <td><p id="pb" name="s1SimpanPB" value="<%= laporan.data.s1.simpan.pb %>"></td>
                                <td><p id="s" name="s1SimpanS" value="<%= laporan.data.s1.simpan.s %>"></td>                              
                              </tr>
                            </tbody>
                          </table>
                      </div>
                </div>
