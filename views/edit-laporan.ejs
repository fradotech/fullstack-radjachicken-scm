                                <td>&emsp;<%= laporan.data.s2.masak.s %></td>                              
                              </tr>
                              <tr>
                                <th scope="row"><small>Laku</small></th>
                                <td><input class="form-control" id="d" name="s2LakuD" value="<%= laporan.data.s2.lakuS2.d %>"></td>
                                <td><input class="form-control" id="pa" name="s2LakuPA" value="<%= laporan.data.s2.lakuS2.pa %>"></td>
                                <td><input class="form-control" id="pb" name="s2LakuPB" value="<%= laporan.data.s2.lakuS2.pb %>"></td>
                                <td><input class="form-control" id="s" name="s2LakuS" value="<%= laporan.data.s2.lakuS2.s %>"></td>                              
                              </tr>
                              <tr>
                                <th scope="row"><strong style="color: blue;">TOTAL LAKU</strong></th>
                                <td>&emsp;<%= laporan.data.s2.laku.d %></td>
                                <td>&emsp;<%= laporan.data.s2.laku.pa %></td>
                                <td>&emsp;<%= laporan.data.s2.laku.pb %></td>
                                <td>&emsp;<%= laporan.data.s2.laku.s %></td>                              
                              </tr>
                              <tr>
                                <th scope="row">Etalase</th>
                                <td>&emsp;<%= laporan.data.s2.etalase.d %></td>
                                <td>&emsp;<%= laporan.data.s2.etalase.pa %></td>
